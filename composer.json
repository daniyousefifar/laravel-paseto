{
    "name": "mydaniel/laravel-paseto",
    "description": "Paseto (Platform-Agnostic Security Tokens) authentication guard for Laravel. A secure alternative to JWT.",
    "keywords": [
        "laravel",
        "paseto",
        "auth",
        "guard",
        "authentication",
        "token",
        "jwt",
        "security"
    ],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Yousefi Far",
            "email": "daniyousefifar@gmail.com",
            "homepage": "https://github.com/daniyousefifar"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/auth": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "paragonie/paseto": "^v3.0"
    },
    "autoload": {
        "psr-4": {
            "MyDaniel\\Paseto\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyDaniel\\Paseto\\PasetoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
